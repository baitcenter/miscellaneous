{
  "name": "miscellaneous",
  "version": "0.0.1",
  "repository": "https://github.com/ntnyq/miscellaneous.git",
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "scripts": {
    "pretty": "prettier --write src/**/*.js",
    "pub": "sh scripts/deploy.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "devDependencies": {
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "sort-package-json": "^1.30.0"
  }
}
